.profile_dash
  = image_tag('tim.jpg')
  - if current_user.membership == 'basic'
    =link_to 'Upgrade to premium', update_membership_path
  - else
    =link_to 'Downgrade to basic', update_membership_path

  %ul#small
    %li= current_user.name
    %li= current_user.gender
    %li= current_user.location
    %li= current_user.age

.inbox
  %h2#incoming Fetching messages...
  = render 'messages/dash'
  #dash_new_message
    = link_to 'Messages', all_path
  

.likes
  %h2#likes Likes
  =render @users_matched

.search
  %h2#search Search
  =render 'shared/search'
